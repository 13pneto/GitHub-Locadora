<Window x:Class="Locadora.Views.Listar.frmListarFilmes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Locadora.Views.Listar"
        mc:Ignorable="d"
        Title="Listar Filmes" Height="450" Width="800">
    <Grid Margin="0,0,-92,-154">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="8*"/>
            <ColumnDefinition Width="433*"/>
            <ColumnDefinition Width="443*"/>
        </Grid.ColumnDefinitions>
        <GroupBox x:Name="groupBox" Header="Filmes Ativos" HorizontalAlignment="Left" Height="335" Margin="48,73,0,0" VerticalAlignment="Top" Width="361" Grid.Column="1">
            <DataGrid x:Name="dtFilmesAtivos" HorizontalAlignment="Left" Height="256" Margin="10,42,0,0" VerticalAlignment="Top" Width="326"/>
        </GroupBox>
        <GroupBox x:Name="groupBox1" Grid.Column="2" Header="Filmes Inativos" HorizontalAlignment="Left" Height="335" Margin="10,73,0,0" VerticalAlignment="Top" Width="363">
            <DataGrid x:Name="dtFilmesInativos" HorizontalAlignment="Left" Height="265" Margin="10,35,-2,0" VerticalAlignment="Top" Width="343"/>
        </GroupBox>
    </Grid>

</Window>
